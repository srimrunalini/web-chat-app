<html lang="en">  
<head>  
    <meta charset="UTF-8">  
    <meta name="viewport" content="width=device-width, initial-scale=1.0">  
    <meta http-equiv="X-UA-Compatible" content="ie=edge">  
    <title>MSIT CSV Uploader</title>  
    <link rel="stylesheet" href="/css/bootstrap.min.css">  
</head>  
<body>  
        <nav class="navbar navbar-expand-lg navbar-dark bg-primary fixed-top">  
                <a class="navbar-brand" href="#">MSIT CSV Converter</a>  
              </nav>  
    <div class="container">  
        <div class=" nav justify-content-center" style="margin-top:100px;">  
         <div class="card border-warning mb-3 " style="max-width: 40rem;">  
                <div class="card-body">  
                  <form action="/" method="get" enctype="multipart/form-data">  
                    <h1>Inserted Succesfully</h1>
        
                            <div class="text-center"><button type="submit" class="btn btn-lg btn-primary">Logout</a></button></div>     
                       </form></div>  
          </div>  
    </div><br>  
    <%if(data){%>  
        <div>  
            <table class="table table-hover table-responsive table-stripped  nav justify-content-center" style="width: auto" >   
                    <thead>  
                        <tr class="bg-primary">  
                            <th>username</th>  
                            <th>password</th>  
                            <th>EmailID</th>  
                            <th style="padding-right: 1em">LastName</th>  
                            <th style="padding-right: 1em">FirstName</th>  
                            <th style="padding-right:2em;padding-left:2em;">Age</th>  
                            <th>Position</th>  
                            <th>Mobile</th>  
                            <th>DOB</th> 
                            <th>year</th> 
                             
                           </tr>  
                    </thead>  
                    <tbody style="overflow-x: scroll; height:350px;" class="table-bordered">  
                        <%for(var i=0;i< data.length;i++){%>  
                        <tr class="text-center">  
                            <td style="padding-left: 1em"><%= data[i].username%></td>  
                            <td style="padding-left: 1.2em"><%= data[i].password%></td>  
                            <td style="padding-right: 1em"><%= data[i].EmailId%></td>  
                                      <td style="padding-left: 1em;"><%= data[i].FirstName%></td>  
                                      <td style="padding-right: 1em"><%= data[i].LastName%></td>  
                                      <td style="padding-right:1em;padding-left:1em;"><%= data[i].Age%></td>  
                                      <td style="padding-left: 1em"><%= data[i].Position%></td>  
                                      <td style="padding-left: 1em"><%= data[i].Mobile%></td>  
                                      <td style="padding-left: 1.2em"><%= data[i].DOB%></td>  
                                      <td style="padding-left: 1.2em"><%= data[i].year%></td>  
                                     </tr>  
                        <%}%>  
                    </tbody>  
            </table>  
        </div>  
        <%}%>  
    
</div>
</body>
</html>